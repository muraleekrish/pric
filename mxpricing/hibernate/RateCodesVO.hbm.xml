<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    
    <class
        name="com.savant.pricing.valueobjects.RateCodesVO"
        table="PRC_Rate_Codes">

        <id
            name="rateCodeIdentifier"
            column="Rate_Code_ID"
            type="int">
            
            <generator class="identity">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-ContextType.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>
		
		<many-to-one name="tdsp" fetch="join" column="TDSP_ID"/>    	
		
		<property
            name="rateCode"
            type="string"
            update="true"
            insert="true"
            column="Rate_Code"
        />
		
		
		
		<property
            name="rateClass"
            type="string"
            update="true"
            insert="true"
            column="Rate_Class"
        />
		
		<property
            name="description"
            type="string"
            update="true"
            insert="true"
            column="Descrptn"
        />
		
		<property
            name="createdBy"
            type="string"
            update="true"
            insert="true"
            column="Created_By"
        />
		
		<property
            name="createdDate"
            type="timestamp"
            update="true"
            insert="true"
            column="Created_Date"
        />
		
		<property
            name="modifiedBy"
            type="string"
            update="true"
            insert="true"
            column="Modified_By"
        />
		
		<property
            name="modifiedDate"
            type="timestamp"
            update="true"
            insert="true"
            column="Modified_Date"
        />
		
		<property
            name="valid"
            type="boolean"
            update="true"
            insert="true"
            column="Is_Valid"
        />
    </class>

</hibernate-mapping>
