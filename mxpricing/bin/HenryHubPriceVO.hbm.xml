<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    
    <class
        name="com.savant.pricing.calculation.valueobjects.HenryHubPriceVO"
        table="PRC_HH_Price">

		<composite-id> 
   		 <key-property name="marketDate" type="date" column="Mrkt_Date"/>
   		 <key-property name="monthYear" type="date" column="Mnth_Yr"/>
  		 <key-many-to-one name="dataSource" class="com.savant.pricing.calculation.valueobjects.ForwardCurvesSourceVO" column="Data_Src_ID"/>    
   		</composite-id>			
		<property
            name="price"
            type="float"
            update="true"
            insert="true"
            column="Price"
        />
    </class>

</hibernate-mapping>
